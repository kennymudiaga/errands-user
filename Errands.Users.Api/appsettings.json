{
  "JwtInfo": {
    "Issuer": "ErrandsNg.Users.Api",
    "Audience": "ErrandsNg",
    "SecretKey": "#{SecretJwtKey}#"
  },
  "UserPolicy": {
    "EnableLockout": true,
    "MaxPasswordFailCount": 5,
    "PasswordLockoutMinutes": 5,
    "TokenExpiryMinutes": 5,
    "SessionTimeout": 1440
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
